<p id="notice"><%= notice %></p>

<h1>Programacion </h1>

<table>
  <thead>
    <tr>
      <th>Fecha1</th>
      <th>Fecha2</th>
      <th>Code</th>
      <th>Bank acount</th>
      <th>Importe</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @telecreditos.each do |telecredito| %>
      <tr>
        <td><%= telecredito.fecha1 %></td>
        <td><%= telecredito.fecha2 %></td>
        <td><%= telecredito.code %></td>
        <td><%= telecredito.bank_acount.bank.name %></td>
        <td><%= telecredito.importe %></td>
        <td><%= link_to 'Ver', telecredito %></td>
        <td><%= link_to 'Editar', edit_telecredito_path(telecredito) %></td>
        <td><%= link_to 'Anular', telecredito, method: :delete, data: { confirm: 'Esta seguro de anular ?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
<div class="button">
<%= link_to 'NTelecredito', new_telecredito_path %>
</div>
