<p id="notice"><%= notice %></p>

<h1>Listing Telecreditos</h1>

<table>
  <thead>
    <tr>
      <th>Fecha1</th>
      <th>Fecha2</th>
      <th>Code</th>
      <th>Bank acount</th>
      <th>Importe</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @telecreditos.each do |telecredito| %>
      <tr>
        <td><%= telecredito.fecha1 %></td>
        <td><%= telecredito.fecha2 %></td>
        <td><%= telecredito.code %></td>
        <td><%= telecredito.bank_acount_id %></td>
        <td><%= telecredito.importe %></td>
        <td><%= link_to 'Show', telecredito %></td>
        <td><%= link_to 'Edit', edit_telecredito_path(telecredito) %></td>
        <td><%= link_to 'Destroy', telecredito, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Telecredito', new_telecredito_path %>
