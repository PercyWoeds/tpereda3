<p id="notice"><%= notice %></p>

<h1>Listing Tanks</h1>

<table>
  <thead>
    <tr>
      <th>Code</th>
      <th>Product</th>
      <th>Saldo inicial</th>
      <th>Varilla</th>
      <th>Product</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tanks.each do |tank| %>
      <tr>
        <td><%= tank.code %></td>
        <td><%= tank.product_id %></td>
        <td><%= tank.saldo_inicial %></td>
        <td><%= tank.varilla %></td>
        <td><%= tank.product_id %></td>
        <td><%= link_to 'Show', tank %></td>
        <td><%= link_to 'Edit', edit_tank_path(tank) %></td>
        <td><%= link_to 'Destroy', tank, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tank', new_tank_path %>
