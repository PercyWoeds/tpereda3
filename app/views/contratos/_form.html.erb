<%= simple_form_for(@contrato) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">


    <%= f.input :employee_id %>


    <%= f.input :fecha_inicio %>
    
    <%= f.input :fecha_fin %>
    <%= f.input :fecha_suscripcion %>
    <%= f.input :location_id %>
    <%= f.input :division_id %>
    <%= f.input :sueldo %>
    <%= f.input :reingreso %>
    <%= f.input :comments %>
    <%= f.input :modalidad_id %>
    <%= f.input :tipocontrato_id %>
    <%= f.input :submodalidad_id %>
    <%= f.input :moneda_id %>
    <%= f.input :tiporemuneracion_id %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
