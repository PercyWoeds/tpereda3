<%= simple_form_for(@rqdetail) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :requerimiento_id %>
    <%= f.input :codigo %>
    <%= f.input :qty %>
    <%= f.input :unidad_id %>
    <%= f.input :descrip %>
    <%= f.input :placa_destino %>
    <%= f.input :string %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
