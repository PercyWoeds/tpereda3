<p id="notice"><%= notice %></p>

<h1>Listing Tipocars</h1>

<table>
  <thead>
    <tr>
      <th>Code</th>
      <th>Nombre</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tipocars.each do |tipocar| %>
      <tr>
        <td><%= tipocar.code %></td>
        <td><%= tipocar.nombre %></td>
        <td><%= link_to 'Show', tipocar %></td>
        <td><%= link_to 'Edit', edit_tipocar_path(tipocar) %></td>
        <td><%= link_to 'Destroy', tipocar, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tipocar', new_tipocar_path %>
