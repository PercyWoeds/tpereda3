<%= simple_form_for(@contactopmdetail) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :contacto %>
    <%= f.input :email %>
    <%= f.input :telefono %>
    <%= f.association :contactopm %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
