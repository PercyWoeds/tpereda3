<p id="notice"><%= notice %></p>

<h1>Listing Products Lines</h1>

<table>
  <thead>
    <tr>
      <th>Code</th>
      <th>Name</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @products_lines.each do |products_line| %>
      <tr>
        <td><%= products_line.code %></td>
        <td><%= products_line.name %></td>
        <td><%= products_line.user_id %></td>
        <td><%= link_to 'Ver', products_line %></td>
        <td><%= link_to 'Editar', edit_products_line_path(products_line) %></td>
        <td><%= link_to 'Eliminar', products_line, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'Nuevo Linea', new_products_line_path %>
